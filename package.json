{
  "name": "foldvscode",
  "displayName": "Fold VSCode",
  "description": "Fold function and folders",
  "version": "0.0.8",
  "repository": "https://github.com/leonatkdev/extension-vscode-",
  "publisher": "LeonatKrasniqi",
  "license": "MIT",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "keywords": [
    "fold",
    "unfold",
    "code folding",
    "collapse",
    "expand",
    "vscode",
    "productivity"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "icon": "resources/vsicon.png",
  "galleryBanner": {
    "color": "#007acc",
    "theme": "dark"
  },
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "foldContainer",
          "title": "Fold",
          "icon": "resources/line.svg"
        }
      ]
    },
    "views": {
      "foldContainer": [
        {
          "id": "foldView",
          "name": "Commands and Keybindings"
        }
      ]
    },
    "commands": [
      {
        "command": "autofold.foldFunctions",
        "title": "Fold Functions"
      },
      {
        "command": "autofold.foldFile",
        "title": "Fold File"
      },
      {
        "command": "autofold.foldFolder",
        "title": "Fold Folders"
      },
      {
        "command": "autofold.unfoldFile",
        "title": "Unfold File"
      },
      {
        "command": "autofold.foldAllExceptSelectedFunc",
        "title": "Fold All Except Selected"
      },
      {
        "command": "autofold.unfoldAllExceptSelectedFunc",
        "title": "Unfold All Except Selected"
      },
      {
        "command": "autofold.foldLevel1",
        "title": "Fold Level 1"
      },
      {
        "command": "autofold.foldLevel2",
        "title": "Fold Level 2"
      },
      {
        "command": "autofold.foldLevel3",
        "title": "Fold Level 3"
      },
      {
        "command": "autofold.foldLevel4",
        "title": "Fold Level 4"
      },
      {
        "command": "autofold.foldLevel5",
        "title": "Fold Level 5"
      },
      {
        "command": "autofold.toggleCheckbox",
        "title": "Toggle Checkbox"
      }
    ],
    "keybindings": [
      {
        "command": "autofold.foldFunctions",
        "key": "ctrl+win+w",
        "when": "editorTextFocus"
      },
      {
        "command": "autofold.foldFile",
        "key": "ctrl+alt+f",
        "when": "editorTextFocus"
      },
      {
        "command": "autofold.foldFolder",
        "key": "ctrl+alt+a",
        "when": "editorTextFocus"
      },
      {
        "command": "autofold.unfoldFile",
        "key": "ctrl+alt+u",
        "when": "editorTextFocus"
      },
      {
        "command": "autofold.foldAllExceptSelectedFunc",
        "key": "ctrl+alt+e",
        "when": "editorTextFocus"
      },
      {
        "command": "autofold.unfoldAllExceptSelectedFunc",
        "key": "ctrl+alt+shift+e",
        "when": "editorTextFocus"
      },
      {
        "command": "autofold.foldLevel1",
        "key": "ctrl+alt+1",
        "when": "editorTextFocus"
      },
      {
        "command": "autofold.foldLevel2",
        "key": "ctrl+alt+2",
        "when": "editorTextFocus"
      },
      {
        "command": "autofold.foldLevel3",
        "key": "ctrl+alt+3",
        "when": "editorTextFocus"
      },
      {
        "command": "autofold.foldLevel4",
        "key": "ctrl+alt+4",
        "when": "editorTextFocus"
      },
      {
        "command": "autofold.foldLevel5",
        "key": "ctrl+alt+5",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "autofold.foldFile",
          "when": "editorTextFocus"
        },
        {
          "command": "autofold.foldFolder",
          "when": "editorTextFocus"
        },
        {
          "command": "autofold.unfoldFile",
          "when": "editorTextFocus"
        },
        {
          "command": "autofold.foldAllExceptSelectedFunc",
          "when": "editorHasSelection"
        },
        {
          "command": "autofold.unfoldAllExceptSelectedFunc",
          "when": "editorHasSelection"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.91.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "ts-loader": "^9.5.1",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
